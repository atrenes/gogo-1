    </changeSet>

    <changeSet id="items" author="postgres">
        <sqlFile path="sql/items.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="inventory" author="postgres">
        <sqlFile path="sql/inventory.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="fights" author="postgres">
        <sqlFile path="sql/fights.sql" relativeToChangelogFile="true"/>
    </changeSet>

</databaseChangeLog>