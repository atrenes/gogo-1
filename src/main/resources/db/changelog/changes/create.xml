        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="status" author="postgres">
        <sqlFile path="sql/status.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="users" author="postgres">
        <sqlFile path="sql/users.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <changeSet id="stands" author="postgres">
